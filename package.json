{
  "name": "pencatatan-keuangan-firebase",
  "private": true,
  "type": "module",
  "scripts": {
    "emulators:start": "firebase emulators:start --project demo-pencatatan-keuangan",
    "test:rules": "firebase emulators:exec --project demo-pencatatan-keuangan \"node --test test/rules\"",
    "deploy:rules": "firebase deploy --only firestore:rules,storage:rules"
  },
  "devDependencies": {
    "@firebase/rules-unit-testing": "^3.0.0",
    "firebase": "^10.12.4",
    "firebase-tools": "^13.24.0"
  }
}
